{
  "version": "1.0.0-*",
  "buildOptions": {
    "compilerName": "fsc",
    "emitEntryPoint": true,
    "compile": {
      "includeFiles": [
        "../FSharpLint.Console/Program.fs"
      ]
    }
  },
  "dependencies": {
    "Microsoft.FSharp.Core.netcore": "1.0.0-alpha-*",
    "FSharp.Compiler.Service.netcore": "1.0.0-alpha-*",
    "Microsoft.NETCore.App": {
        "type": "platform",
        "version": "1.0.0"
    },
    "FSharpLint.Core.netcore":{
      "target":"project",
      "type": "build"
    }
  },
  "tools": {
    "dotnet-compile-fsc": {
      "version": "1.0.0-preview2-*",
      "imports": [
        "dnxcore50",
        "portable-net45+win81"
      ]
    },
    "dotnet-mergenupkg": { "version": "1.0.*" }
  },
  "frameworks": {
    "netcoreapp1.0": {
      "imports": [
        "portable-net45+win8",
        "dnxcore50"
      ]
    }
  }
}